    <script>
        function loadDetails() {
            fetch("/studentDetails")
                .then(res => res.text())
                .then(data => {
                    document.getElementById("content").innerHTML = data;
                });
        }


        function loadRegister() {
    fetch("/registerForm")
        .then(res => res.text())
        .then(data => {
            document.getElementById("content").innerHTML = data;
        });
}
